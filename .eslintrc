{
  "rules": {
    "arrow-parens": 0,
    "no-shadow": 0,
    "no-use-before-define": 0,
    "semi": [
      "error",
      "never"
    ],
    "comma-dangle": 0,
    "no-sequences": 0,
    "no-confusing-arrow": 0,
    "func-call-spacing": 0,
    "import/no-unresolved": 0,
    "import/no-extraneous-dependencies": 0,
    "import/extensions": 0,
    "indent": 0
  },
  "globals": {
    "it": true,
    "describe": true
  }
}